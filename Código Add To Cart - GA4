items: [{	
item_name: ´[[nome do produto]]´	
item_id: ´[[id do produto]]´,	
price: ´[[preco]]´,	
item_brand: ´[[marca]]´,	
item_category: ´[[categoria do produto]]´,	
item_variant: ´[[variante do produto]]´,	
quantity: ´[[quantidade]]´	
}]	
}	
});	
</script>	
</HTML>	
